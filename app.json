{
  "expo": {
    "name": "MD App",
    "slug": "md-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "mailer-daemon",
    "icon": "./assets/md_logo.png",
    "userInterfaceStyle": "light",
    "scheme": "appmailerdaemon",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.mdapp"
    },
    "android": {
      "targetSdkVersion": 35,
      "versionCode": 10,
      "package": "com.yourcompany.mdapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/md_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            { "scheme": "appmailerdaemon" },
            { "scheme": "https", "host": "clerk.appmailerdaemon.online" }
          ]
        }
      ]
    },
    "plugins": [
      "expo-image-picker",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/md_logo.png",
          "color": "#FF6600",
          "mode": "production"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "extraParams": {
              "queries": [
                { "intent": { "action": "android.intent.action.VIEW", "data": { "scheme": "https" } } },
                { "intent": { "action": "android.intent.action.VIEW", "data": { "scheme": "http" } } }
              ]
            }
          }
        }
      ]
    ],
    "extra": {
      "eas": { "projectId": "67ca0aa9-492d-41f1-bbdb-6f8c16d7b345" }
    }
  }
}